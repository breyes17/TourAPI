{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "spaced-comment": "off",
        "no-console": "warn",
        "consistent-return":"off",
        "func-names":"off",
        "no-unused-vars":["error", {"argsIngorePattern": "req|res|next|val"}]
    }
}